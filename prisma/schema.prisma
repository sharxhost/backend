generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Image {
  shortid   String   @unique @db.Char(8)
  uuid      String   @id @db.Uuid @default(uuid())
  name      String
  uploaded  DateTime @default(now())
  size      Int
  hash      String   @db.Char(64)
}
